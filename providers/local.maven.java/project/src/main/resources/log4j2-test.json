{
    "configuration": {
        "status": "error",
        "name": "Kodomondo-LocalMavenServer",
        "packages": "org.apache.logging.log4j.test",
        "ThresholdFilter": {
            "level": "all"
        },
        "appenders": {
            "Console": {
                "name": "STDOUT",
                "PatternLayout": {
                    "pattern": "%d{ABSOLUTE} - %-5p (%c{1}) [%X] - %m%n"
                }
            }
        },
        "loggers": {
            "logger": [
                {
                    "name": "EventLogger",
                    "level": "info",
                    "additivity": "false",
                    "appender-ref": {
                        "ref": "Routing"
                    }
                },
                {
                    "name": "com.andrewregan",
                    "level": "debug",
                    "additivity": "true"
                },
                {
                    "name": "net.myrrix",
                    "level": "info",
                    "additivity": "true"
                }
            ],
            "root": {
                "level": "debug",
                "appender-ref": {
                    "ref": "STDOUT"
                }
            }
        }
    }
}