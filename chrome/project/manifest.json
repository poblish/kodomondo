{
    "name": "kodomondo",
    "manifest_version": 2,
    "version": "1.0.0.0",
    "description": "tbc",
    "homepage_url": "http://www.andrewregan.com",
    "options_page": "options/options.html",
    "background": {
        "scripts": ["background/background.js"]
    },
    "icons": {},
    "browser_action": {
        "default_icon": "img/icon19.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css": [
                "css/kodomondo.css"
            ],
            "js": [
                "js/kodomondo.js"
            ]
        }
    ],
    "web_accessible_resources": ["img/blank.png"],
    "permissions": [
        "tabs",
        "notifications"
    ],
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}